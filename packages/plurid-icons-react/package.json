{
    "name": "@plurid/plurid-icons-react",
    "version": "0.0.0-4",
    "description": "Plurid Icons for Plurid Applications based on React.",
    "keywords": [
        "plurid",
        "icons",
        "react"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid-icons"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/plurid-icons/issues"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid-icons/blob/master/packages/plurid-icons-react/README.md",
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "lint": "eslint ./source --ext .ts,.tsx -c ./configurations/.eslintrc.js",
        "test": "jest -c ./configurations/jest.config.js --coverage",
        "clean": "rm -rf ./distribution",
        "start": "pnpm clean && rollup -c ./scripts/rollup.config.ts -w --environment ENV_MODE:local",
        "build.clean": "rm -rf `find ./distribution/ -type d -name __tests__` && rm -rf `find ./distribution/ -type d -name __stories__` && rm -rf ./distribution/utilities/storybook",
        "build.development": "pnpm clean && rollup -c ./scripts/rollup.config.ts --environment ENV_MODE:local",
        "build.production": "pnpm clean && rollup -c ./scripts/rollup.config.ts --environment ENV_MODE:production",
        "build": "pnpm clean && pnpm lint && pnpm test && pnpm build.production && pnpm build.clean",
        "prepublishOnly": "pnpm build"
    },
    "peerDependencies": {
        "@plurid/plurid-themes": "*",
        "react": ">=17",
        "react-dom": ">=17",
        "styled-components": ">=5"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@plurid/plurid-themes": "^0.0.0-2",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.3",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.25",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "babel-runtime": "^6.26.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.21.0",
        "jest": "^28.1.3",
        "jest-config": "^28.1.3",
        "node-fetch": "^3.2.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-test-renderer": "^18.2.0",
        "rollup": "^2.77.2",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-typescript2": "^0.32.1",
        "styled-components": "^5.3.5",
        "ts-jest": "v28.0.7",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.13",
        "typescript": "^4.7.4"
    }
}
